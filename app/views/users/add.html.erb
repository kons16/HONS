<div class='itemposition'>
  <div id="search-box">
    <%= form_tag(add_user_path(current_user), method: :get) do %>
        <%= text_field_tag :book, "",  id: "book_add", name: "keyword", placeholder: "キーワードを打ち込んでください", style: "width: 200px;"%>
        <button title="検索" type="submit">検索</button>

    <% end %>
  </div>
  <h2>検索結果</h2>
  <div id='book_list'>
   <%= render partial: 'book_list' %>
  </div>
</div>